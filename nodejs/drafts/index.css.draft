@import "../www/css/my-fonts.css";
@media only screen and (min-width: 1020px) {
  body {
    margin: 0;
    color: #fff;
    background-color: #000;
  }
  #title_bar {
    display: flex;
    line-height: 1em;
  }
  #title_bar #title {
    flex: 5;
    padding-left: 1.5%;
    background-color: #000;
  }
  #title_bar h1 {
    color: #f00;
    text-align: left;
    font-family: "Lintsec", sans-serif;
    text-shadow: 0 0 2px #ffa500;
    font-size: xx-large;
    letter-spacing: 5px;
  }
  #title_bar h2 {
    color: #fff;
    text-align: left;
    font-family: "Lintsec", sans-serif;
    font-size: x-large;
    letter-spacing: 1px;
  }
  #title_bar aside {
    margin-top: auto;
    margin-bottom: auto;
    background-color: #000;
    flex: 2;
    padding: 1%;
    text-align: right;
    font-size: large;
    margin-right: 1%;
    background: -webkit-linear-gradient(left, #ff0900, #000, #000); /* For Safari 5.1 to 6.0 */
    background: -o-linear-gradient(right, #ff0900, #000, #000); /* For Opera 11.1 to 12.0 */
    background: -moz-linear-gradient(right, #ff0900, #000, #000); /* For Firefox 3.6 to 15 */
    background: linear-gradient(to right, #ff0900, #000, #000); /* Standard syntax */
    border-left: 1px solid #ff0900;
    border-radius: 8px;
  }
  #title_bar h4 {
    line-height: 1em;
    margin: 0;
    font-family: "Roboto-Bold", sans-serif;
    font-size: medium;
  }
  #title_bar h4 a#email {
    color: #fff;
    font-family: "Bookman", Georgia, "Times New Roman", serif;
  }
  #subtitle {
    background-color: #ff0900;
/*border-top 1px solid orange*/
/*border-bottom 1px solid orange*/
/*background: -webkit-linear-gradient(0deg, #ff6c00, background-color-subtitle, #ff6c00); */
/* For Safari 5.1 to 6.0 */
/*background: -o-linear-gradient(0deg, #ff6c00, background-color-subtitle, #ff6c00); */
/* For Opera 11.1 to 12.0 */
/*background: -moz-linear-gradient(0deg, #ff6c00, background-color-subtitle, #ff6c00); */
/* For Firefox 3.6 to 15 */
/*background: linear-gradient(0deg, #ff6c00, background-color-subtitle, #ff6c00); */
/* Standard syntax */
    line-height: 1em;
    color: #fff;
    text-align: center;
    font-family: "Lintsec", sans-serif;
    margin: 0;
    margin-bottom: 1%;
    padding: 1%;
    font-size: x-large;
    letter-spacing: 5px;
  }
  #content {
    display: flex;
  }
  #content nav {
    flex: 2;
    background-color: #000;
    text-align: center;
    list-style-type: none;
    margin: 1%;
  }
  #content nav img {
    height: 100%;
    width: 85%;
    border: 1px solid #f00;
/*box-shadow box-shadow-orange*/
  }
  #content nav div {
    background-color: #d3d3d3;
    border-radius: 4px;
    padding: 1%;
    margin-top: 3%;
    width: 100%;
  }
  #content article {
    flex: 3;
    padding: 2% 4% 2% 4%;
    text-align: justify;
    font-family: "Bookman", Georgia, "Times New Roman", serif;
    font-size: medium;
    background-color: #000;
  }
  #content aside {
    flex: 2;
    padding: 1%;
    margin: 0 auto;
    text-align: left;
  }
  #content aside div > h3 {
    border: 1px solid #d3d3d3;
    border-radius: 4px;
    padding: 1%;
    background-color: aside-background-color;
    font-family: "Bookman", Georgia, "Times New Roman", serif;
    text-shadow: 0 0 2px #000;
  }
  label {
    color: #808080;
    padding: 1%;
    text-shadow: 0 0 2px #000;
    font-family: "RockSalt", sans-serif;
    font-size: large;
  }
  input {
    padding: 2%;
    margin-top: 4%;
  }
  input[type=submit] {
    border-radius: 4px;
    text-align: center;
    font-family: "Bookman", Georgia, "Times New Roman", serif;
    width: 50%;
  }
  a:link {
    text-decoration: none;
    color: #b22222;
    text-shadow: 0 0 2px #f00;
    font-family: "Bookman", Georgia, "Times New Roman", serif;
    font-size: medium;
  }
  a:visited {
    text-decoration: none;
    color: #b22222;
    text-shadow: 0 0 2px #f00;
    font-family: "Bookman", Georgia, "Times New Roman", serif;
    font-size: medium;
  }
}
